			Pseq([~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4]], 1), // Line 10
			Pseq([~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~fsharp2, [~a3, ~csharp4, ~fsharp4]], 1), // Line 11
			Pseq([~b1, [~b3, ~d4, ~fsharp4], ~e2, [~g3, ~b3], ~e2, [~b3, ~d4, ~g4], ~d2, [~f3, ~a3, ~d4], ~a1, [~a3, ~c4, ~e4], ~d2, [~g3, ~b3, ~e4]], 1), // Line 12
			Pseq([~d2, [~d3, ~g3, ~b3, ~e4], ~d2, [~c3, ~e3, ~a3, ~d4], ~d2, [~c3, ~fsharp3, ~a3, ~d4], ~d2, [~a3, ~c4, ~f4], ~d2, [~a3, ~c4, ~e4]], 1), // Line 13
			Pseq([~d2, [~d3, ~g3, ~b3, ~e4], ~d2, [~c3, ~e3, ~a3, ~d4], ~d2, [~c3, ~fsharp3, ~a3, ~d4], ~e2, [~b3, ~e4, ~g4], ~e2, [~b3, ~d4, ~f4, ~a4], ~e2, [~b3, ~c4, ~f4]], 1), // Line 14
			Pseq([~e2, [~c3, ~e3, ~a3], ~e2, [~a3, ~csharp4, ~fsharp4, ~a4], ~e2, [~b2, ~a3, ~d4], [~e3, ~b3, ~d4, ~g4], [~a2, ~g3], [~d2, ~a2, ~b2]], 1)], inf),// Line 15
	\amp, Pexprand(65,82,inf)/127,
	\dur, Pseq([
			Pseq([1/4, 1/2, 1/4, 1/2, 1/4, 1/2, 1/4, 1/2, 1/4, 1/2], 1), // Line 1
