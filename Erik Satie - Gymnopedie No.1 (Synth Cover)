	centbank15 = Select.kr(cent15, [DC.ar(0), ~pos1cents, ~pos2cents, ~pos3cents, ~pos4cents, ~pos5cents, ~pos6cents, ~pos7cents, ~pos8cents, ~pos9cents, ~pos10cents, ~pos11cents, ~pos12cents, ~pos13cents, ~pos14cents, ~pos15cents, ~pos16cents, ~pos17cents, ~pos18cents, ~pos19cents, ~pos20cents, ~pos21cents, ~pos22cents, ~pos23cents, ~pos24cents, ~pos25cents, ~pos26cents, ~pos27cents, ~pos28cents, ~pos29cents, ~pos30cents, ~pos31cents, ~pos32cents, ~pos33cents, ~pos34cents, ~pos35cents, ~pos36cents, ~pos37cents, ~pos38cents, ~pos39cents, ~pos40cents, ~pos41cents, ~pos42cents, ~pos43cents, ~pos44cents, ~pos45cents, ~pos46cents, ~pos47cents, ~pos48cents, ~pos49cents, ~pos50cents, ~pos51cents, ~pos52cents, ~pos53cents, ~pos54cents, ~pos55cents, ~pos56cents, ~pos57cents, ~pos58cents, ~pos59cents, ~pos60cents, ~pos61cents, ~pos62cents, ~pos63cents, ~pos64cents, ~pos65cents, ~pos66cents, ~pos67cents, ~pos68cents, ~pos69cents, ~pos70cents, ~pos71cents, ~pos72cents, ~pos73cents, ~pos74cents, ~pos75cents, ~pos76cents, ~pos77cents, ~pos78cents, ~pos79cents, ~pos80cents, ~pos81cents, ~pos82cents, ~pos83cents, ~pos84cents, ~pos85cents, ~pos86cents, ~pos87cents, ~pos88cents, ~pos89cents, ~pos90cents, ~pos91cents, ~pos92cents, ~pos93cents, ~pos94cents, ~pos95cents]);
	centbank16 = Select.kr(cent16, [DC.ar(0), ~neg1cent, ~neg2cents, ~neg3cents, ~neg4cents, ~neg5cents, ~neg6cents, ~neg7cents, ~neg8cents, ~neg9cents, ~neg10cents, ~neg11cents, ~neg12cents, ~neg13cents, ~neg14cents, ~neg15cents, ~neg16cents, ~neg17cents, ~neg18cents, ~neg19cents, ~neg20cents, ~neg21cents, ~neg22cents, ~neg23cents, ~neg24cents, ~neg25cents, ~neg26cents, ~neg27cents, ~neg28cents, ~neg29cents, ~neg30cents, ~neg31cents, ~neg32cents, ~neg33cents, ~neg34cents, ~neg35cents, ~neg36cents, ~neg37cents, ~neg38cents, ~neg39cents, ~neg40cents, ~neg41cents, ~neg42cents, ~neg43cents, ~neg44cents, ~neg45cents, ~neg46cents, ~neg47cents, ~neg48cents, ~neg49cents, ~neg50cents, ~neg51cents, ~neg52cents, ~neg53cents, ~neg54cents, ~neg55cents, ~neg56cents, ~neg57cents, ~neg58cents, ~neg59cents, ~neg60cents, ~neg61cents, ~neg62cents, ~neg63cents, ~neg64cents, ~neg65cents, ~neg66cents, ~neg67cents, ~neg68cents, ~neg69cents, ~neg70cents, ~neg71cents, ~neg72cents, ~neg73cents, ~neg74cents, ~neg75cents, ~neg76cents, ~neg77cents, ~neg78cents, ~neg79cents, ~neg80cents, ~neg81cents, ~neg82cents, ~neg83cents, ~neg84cents, ~neg85cents, ~neg86cents, ~neg87cents, ~neg88cents, ~neg89cents, ~neg90cents, ~neg91cents, ~neg92cents, ~neg93cents, ~neg94cents, ~neg95cents]);

	// 2 VOICE UNISON
	voiceonea = Select.ar(whichsigforunison, [Saw.ar(centbank1*freq, amp), Pulse.ar(centbank1*freq, width, amp)]);
	voiceoneb = Select.ar(whichsigforunison, [Saw.ar(centbank1*freq, amp), Pulse.ar(centbank1*freq, width, amp)]);
	voiceonesubosc = Select.ar(whichsigforunison, [Saw.ar(centbank1*freq/2, amp), Pulse.ar(centbank1*freq/2, width, amp)]);
	voiceonesum = voiceonea + voiceoneb + voiceonesubosc;
	voiceonesum = Pan2.ar(voiceonesum, Saw.kr(1).range(-0.8, -0.1));

	voicetwoa = Select.ar(whichsigforunison, [Saw.ar(centbank2*freq, amp), Pulse.ar(centbank2*freq, width, amp)]);
	voicetwob = Select.ar(whichsigforunison, [Saw.ar(centbank2*freq, amp), Pulse.ar(centbank2*freq, width, amp)]);
	voicetwosubosc = Select.ar(whichsigforunison, [Saw.ar(centbank2*freq/2, amp), Pulse.ar(centbank2*freq/2, width, amp)]);
	voicetwosum = voicetwoa + voicetwob + voicetwosubosc;
	voicetwosum = Pan2.ar(voicetwosum, Saw.kr(0.8).range(-0.1, 0.5));

	twovoiceunison = voiceonesum + voicetwosum + noise;

	// 4 VOICE UNISON
	voicethreea = Select.ar(whichsigforunison, [Saw.ar(centbank3*freq, amp), Pulse.ar(centbank3*freq, width, amp)]);
	voicethreeb = Select.ar(whichsigforunison, [Saw.ar(centbank3*freq, amp), Pulse.ar(centbank3*freq, width, amp)]);
	voicethreesubosc = Select.ar(whichsigforunison, [Saw.ar(centbank3*freq/2, amp), Pulse.ar(centbank3*freq/2, width, amp)]);
	voicethreesum = voicethreea + voicethreeb + voicethreesubosc;

	voicefoura = Select.ar(whichsigforunison, [Saw.ar(centbank4*freq, amp), Pulse.ar(centbank4*freq, width, amp)]);
	voicefourb = Select.ar(whichsigforunison, [Saw.ar(centbank4*freq, amp), Pulse.ar(centbank4*freq, width, amp)]);
	voicefoursubosc = Select.ar(whichsigforunison, [Saw.ar(centbank4*freq/2, amp), Pulse.ar(centbank4*freq/2, width, amp)]);
	voicefoursum = voicefoura + voicefourb + voicefoursubosc;

	fourvoiceunison = voiceonesum + voicetwosum + voicethreesum + voicefoursum + noise;

	// 8 VOICE UNISON
	voicefivea = Select.ar(whichsigforunison, [Saw.ar(centbank5*freq, amp), Pulse.ar(centbank5*freq, width, amp)]);
	voicefiveb = Select.ar(whichsigforunison, [Saw.ar(centbank5*freq, amp), Pulse.ar(centbank5*freq, width, amp)]);
	voicefivesubosc = Select.ar(whichsigforunison, [Saw.ar(centbank5*freq/2, amp), Pulse.ar(centbank5*freq/2, width, amp)]);
	voicefivesum = voicefivea + voicefiveb + voicefivesubosc;

	voicesixa = Select.ar(whichsigforunison, [Saw.ar(centbank6*freq, amp), Pulse.ar(centbank6*freq, width, amp)]);
	voicesixb = Select.ar(whichsigforunison, [Saw.ar(centbank6*freq, amp), Pulse.ar(centbank6*freq, width, amp)]);
	voicesixsubosc = Select.ar(whichsigforunison, [Saw.ar(centbank6*freq/2, amp), Pulse.ar(centbank6*freq/2, width, amp)]);
	voicesixsum = voicesixa + voicesixb + voicesixsubosc;

	voicesevena = Select.ar(whichsigforunison, [Saw.ar(centbank7*freq, amp), Pulse.ar(centbank7*freq, width, amp)]);
	voicesevenb = Select.ar(whichsigforunison, [Saw.ar(centbank7*freq, amp), Pulse.ar(centbank7*freq, width, amp)]);
	voicesevensubosc = Select.ar(whichsigforunison, [Saw.ar(centbank7*freq/2, amp), Pulse.ar(centbank7*freq/2, width, amp)]);
	voicesevensum = voicesevena + voicesevenb + voicesevensubosc;

	voiceeighta = Select.ar(whichsigforunison, [Saw.ar(centbank8*freq, amp), Pulse.ar(centbank8*freq, width, amp)]);
	voiceeightb = Select.ar(whichsigforunison, [Saw.ar(centbank8*freq, amp), Pulse.ar(centbank8*freq, width, amp)]);
	voiceeightsubosc = Select.ar(whichsigforunison, [Saw.ar(centbank8*freq/2, amp), Pulse.ar(centbank8*freq/2, width, amp)]);
	voiceeightsum = voiceeighta + voiceeightb + voiceeightsubosc;

	eightvoiceunison = voiceonesum + voicetwosum + voicethreesum + voicefoursum + voicefivesum + voicesixsum + voicesevensum + voiceeightsum + noise;

	// 16 VOICE UNISON
	voiceninea = Select.ar(whichsigforunison, [Saw.ar(centbank9*freq, amp), Pulse.ar(centbank9*freq, width, amp)]);
	voicenineb = Select.ar(whichsigforunison, [Saw.ar(centbank9*freq, amp), Pulse.ar(centbank9*freq, width, amp)]);
	voiceninesubosc = Select.ar(whichsigforunison, [Saw.ar(centbank9*freq/2, amp), Pulse.ar(centbank9*freq/2, width, amp)]);
	voiceninesum = voiceninea + voicenineb + voiceninesubosc;

	voicetena = Select.ar(whichsigforunison, [Saw.ar(centbank10*freq, amp), Pulse.ar(centbank10*freq, width, amp)]);
	voicetenb = Select.ar(whichsigforunison, [Saw.ar(centbank10*freq, amp), Pulse.ar(centbank10*freq, width, amp)]);
	voicetensubosc = Select.ar(whichsigforunison, [Saw.ar(centbank10*freq/2, amp), Pulse.ar(centbank10*freq/2, width, amp)]);
	voicetensum = voicetena + voicetenb + voicetensubosc;

	voiceelevena = Select.ar(whichsigforunison, [Saw.ar(centbank11*freq, amp), Pulse.ar(centbank11*freq, width, amp)]);
	voiceelevenb = Select.ar(whichsigforunison, [Saw.ar(centbank11*freq, amp), Pulse.ar(centbank11*freq, width, amp)]);
	voiceelevensubosc = Select.ar(whichsigforunison, [Saw.ar(centbank11*freq/2, amp), Pulse.ar(centbank11*freq/2, width, amp)]);
	voiceelevensum = voiceelevena + voiceelevenb + voiceelevensubosc;

	voicetwelvea = Select.ar(whichsigforunison, [Saw.ar(centbank12*freq, amp), Pulse.ar(centbank12*freq, width, amp)]);
	voicetwelveb = Select.ar(whichsigforunison, [Saw.ar(centbank12*freq, amp), Pulse.ar(centbank12*freq, width, amp)]);
	voicetwelvesubosc = Select.ar(whichsigforunison, [Saw.ar(centbank12*freq/2, amp), Pulse.ar(centbank12*freq/2, width, amp)]);
	voicetwelvesum = voicetwelvea + voicetwelveb + voicetwelvesubosc;

	voicethirteena = Select.ar(whichsigforunison, [Saw.ar(centbank13*freq, amp), Pulse.ar(centbank13*freq, width, amp)]);
	voicethirteenb = Select.ar(whichsigforunison, [Saw.ar(centbank13*freq, amp), Pulse.ar(centbank13*freq, width, amp)]);
	voicethirteensubosc = Select.ar(whichsigforunison, [Saw.ar(centbank13*freq/2, amp), Pulse.ar(centbank13*freq/2, width, amp)]);
	voicethirteensum = voicethirteena + voicethirteenb + voicethirteensubosc;

	voicefourteentha = Select.ar(whichsigforunison, [Saw.ar(centbank14*freq, amp), Pulse.ar(centbank14*freq, width, amp)]);
	voicefourteenthb = Select.ar(whichsigforunison, [Saw.ar(centbank14*freq, amp), Pulse.ar(centbank14*freq, width, amp)]);
	voicefourteenthsubosc = Select.ar(whichsigforunison, [Saw.ar(centbank14*freq/2, amp), Pulse.ar(centbank14*freq/2, width, amp)]);
	voicefourteenthsum = voicefourteentha + voicefourteenthb + voicefourteenthsubosc;

	voicefifteentha = Select.ar(whichsigforunison, [Saw.ar(centbank15*freq, amp), Pulse.ar(centbank15*freq, width, amp)]);
	voicefifteenthb = Select.ar(whichsigforunison, [Saw.ar(centbank15*freq, amp), Pulse.ar(centbank15*freq, width, amp)]);
	voicefifteenthsubosc = Select.ar(whichsigforunison, [Saw.ar(centbank15*freq/2, amp), Pulse.ar(centbank15*freq/2, width, amp)]);
	voicefifteenthsum = voicefifteentha + voicefifteenthb + voicefifteenthsubosc;

	voicesixteentha = Select.ar(whichsigforunison, [Saw.ar(centbank16*freq, amp), Pulse.ar(centbank16*freq, width, amp)]);
	voicesixteenthb = Select.ar(whichsigforunison, [Saw.ar(centbank16*freq, amp), Pulse.ar(centbank16*freq, width, amp)]);
	voicesixteenthsubosc = Select.ar(whichsigforunison, [Saw.ar(centbank16*freq/2, amp), Pulse.ar(centbank16*freq/2, width, amp)]);
	voicesixteenthsum = voicesixteentha + voicesixteenthb + voicesixteenthsubosc;

	sixteenvoiceunison = voiceonesum + voicetwosum + voicethreesum + voicefoursum + voicefivesum + voicesixsum + voicesevensum + voiceeightsum + voiceninesum + voicetensum + voiceelevensum + voicetwelvesum + voicethirteensum + 	voicefourteenthsum +  voicefifteenthsum + voicesixteenthsum + noise;


	// UNISON SELECTION
	unisonselect = Select.ar(whichunison, [twovoiceunison, fourvoiceunison, eightvoiceunison, sixteenvoiceunison]);

	// LFO MODULE - AMPLITUDE
	amplfo = unisonselect*Select.ar(whichlfo1,
		[DC.ar(0),
			LFSaw.ar(lfoval1, 0, 0.6),
			LFTri.ar(lfoval1),
			LFPulse.ar(lfoval1),
			SinOsc.ar(lfoval1)]);

	// LFO MODULE - CUTOFF
	cutofflfo = Select.ar(whichlfo2,
		[DC.ar(0),
			BLowPass.ar(unisonselect, LFTri.kr(lfoCycle, 0, lfoFreq1, 2040)),
			BHiPass.ar(unisonselect, LFTri.kr(1, 0, 2000, 2040)),
			BBandPass.ar(unisonselect, LFTri.kr(1, 0, 2000, 2040))]);

	// FILTER MODULE
	// FILTER SELECTION
	selectfilter = Select.ar(which2,
		[unisonselect,
		    BLowPass.ar(unisonselect, freqfilter),
		    BHiPass.ar(unisonselect, freqfilter),
		    BBandPass.ar(unisonselect, freqfilter)]);

	// FILTER ENVELOPE
	selectfilter2 = Select.ar(which3,
	 [unisonselect,
		BLowPass.ar(unisonselect, Line.ar(100, 3000, 2.5)),
		BHiPass.ar(unisonselect, EnvGen.kr(Env([5000, 100], [time]))),
		BBandPass.ar(unisonselect, EnvGen.kr(Env([100, 5000], [time])))]);

	mainsig = Select.ar(whichmainsig, [unisonselect, amplfo, cutofflfo, selectfilter, selectfilter2]);

	dist = CrossoverDistortion.ar(mainsig, ampdist, smooth);

	chain = FFT(LocalBuf(bufSize), dist);
	chain = PV_MagFreeze(chain, MouseY.kr > 0.98);
	Out.ar(out, env*IFFT(chain)!2*mainamp);
}).load;
)



(
~gymno1 = Ppar([
~synthrhythm = Pbind(
	\instrument, \unison,
	\freq, Pseq([
			Pseq([~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~g2, [~b3, ~d4, ~fsharp4], ~d2, [~a3, ~csharp4, ~fsharp4], ~g2, [~b3, ~d4, ~fsharp4]], 1),  // Line 1
